{
	"extends": [
		"eslint-config-react",
		"eslint-config-prettier",
	],
	
	"env": {
		"es6": true,
		"browser": true,
		"node": true,
		"jest": true
	},

	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion:": 2016,
		"sourceType": "module",
		"ecmaFeatures": { "jsx": true }
	},
	
	"rules": {
		 "new-cap": ["error", { "newIsCap": false, "capIsNew": false }],
		 "react/sort-comp": [0],
		 "react/prop-types": 0,
		 "eqeqeq": 0
	}
}








